<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Time</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |f| %>
      <tr>
        <td><%= f.description %></td>
        <td><%= f.event_date %></td>
        <td><%= link_to "Show", events %></td>
        <td><%= link_to "Edit", edit_event_path(f) %></td>
        <td><%= link_to "Destroy", f, method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New book", new_event_path %>