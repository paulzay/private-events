<h1><%= @user.name %>'s Events</h1>

<h3>My Events</h3>

<% @user.created_events.each do |event| %>
  <%= link_to event.name, event %>, created by <%= event.creator.name %>
<% end %>




<p>Attended Events:<P>
<% @past_events.each do |event| %>
	<%= link_to event.name, event %>, created by <%= event.creator.name %>
<% end %>
<p>Upcoming Events:<p>
<% @upcoming_events.each do |event| %>
	<%= link_to event.name, event %>, created by <%= event.creator.name %>
<% end %>
